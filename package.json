{
  "name": "stackvo-ui",
  "version": "1.0.0",
  "description": "Stackvo Modern Web Dashboard - Node.js + Express.js + Vue.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production node server/index.js",
    "start:dev": "npm run dev"
  },
  "keywords": ["stackvo", "docker", "api", "vue", "dashboard"],
  "author": "Stackvo Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "dockerode": "^4.0.2",
    "node-pty": "^1.0.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "node-cache": "^5.1.2",
    "vue": "^3.4.15",
    "vuetify": "^3.5.1",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "axios": "^1.6.5",
    "socket.io-client": "^4.6.1",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "@mdi/font": "^7.4.47"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "@vitejs/plugin-vue": "^5.0.3",
    "vite": "^5.0.11",
    "vite-plugin-vuetify": "^2.0.1",
    "sass": "^1.69.7",
    "concurrently": "^8.2.2"
  }
